    <script>
        // Check if loaded in presenter view (iframe) or if user wants presenter mode
        const urlParams = new URLSearchParams(window.location.search);
        const isPresenterView = urlParams.get('view') === 'presenter';
        const initialSlide = parseInt(urlParams.get('slide')) || 0;
        const requestedMode = urlParams.get('mode');

        // If user requested presenter mode, redirect to presenter.html
        if (requestedMode === 'presenter' && !isPresenterView) {
            window.location.href = `presenter.html?slide=${initialSlide}`;
        }

        let currentSlide = initialSlide;
        const slides = document.querySelectorAll('.slide');
        const totalSlides = slides.length;
        const navbar = document.getElementById('navbar');
        const slideCounter = document.getElementById('slideCounter');

        // Sync channel for communication between projector and presenter windows
        let syncChannel = null;

        try {
            syncChannel = new BroadcastChannel('presentation-sync');
            console.log('âœ… BroadcastChannel created successfully');

            // Listen for slide changes from other windows
            syncChannel.onmessage = (event) => {
                console.log('ðŸ“¡ Received message:', event.data);
                if (event.data.type === 'slide-change' && event.data.slide !== currentSlide) {
                    console.log('ðŸ”„ Syncing to slide:', event.data.slide);
                    currentSlide = event.data.slide;
                    updateSlide();
                }
            };
        } catch (error) {
            console.error('âŒ BroadcastChannel not supported:', error);
        }

        // Hide UI elements in presenter iframe view
        if (isPresenterView) {
            console.log('Presenter view mode detected - hiding navbar and controls');

            // Force hide navbar with !important
            if (navbar) {
                navbar.style.setProperty('display', 'none', 'important');
                navbar.style.setProperty('visibility', 'hidden', 'important');
                navbar.style.setProperty('opacity', '0', 'important');
                navbar.style.setProperty('height', '0', 'important');
                navbar.style.setProperty('overflow', 'hidden', 'important');
            }

            if (slideCounter) slideCounter.style.display = 'none';
            const notesBtn = document.getElementById('notesToggle');
            if (notesBtn) notesBtn.style.display = 'none';
            const notesPanel = document.getElementById('notesPanel');
            if (notesPanel) notesPanel.style.display = 'none';

            // Also hide any other UI elements that might appear
            document.body.style.paddingTop = '0';
        }

        // Define sections for each presentation separately
        // Sections will be selected dynamically based on slide's data-presentation attribute

        const oracleSections = [
            { start: 0, end: 1, name: '' },      // Intro: Title + Agenda (no button)
            { start: 2, end: 7, name: 'Company & Team' },          // Divider + slides 3-8
            { start: 8, end: 15, name: 'Problem & Solution' },     // Divider + slides 9-16
            { start: 16, end: 21, name: 'Project Management' },    // Divider + slides 17-22
            { start: 22, end: 39, name: 'Technical Implementation' }, // Divider + slides 23-40
            { start: 40, end: 45, name: 'Results & Validations' }, // Divider + slides 41-46
            { start: 46, end: 49, name: 'Future Enhancements' }    // Divider + slides 47-50
        ];

        const bookSections = [
            { start: 50, end: 51, name: '' },      // Intro: Title + Agenda (no button)
            { start: 52, end: 56, name: 'Introduction' },       // Divider + slides 53-57
            { start: 57, end: 59, name: 'System Conception' },  // Divider + slides 58-60
            { start: 60, end: 62, name: 'Workflow' },           // Divider + slides 61-63
            { start: 63, end: 68, name: 'Implementation' },     // Divider + slides 64-69
            { start: 69, end: 69, name: 'Results' },            // Divider (slide 70)
            { start: 70, end: 74, name: 'Conclusion' }          // Divider + slides 71-75
        ];

        // Current active sections (will be set dynamically based on data-presentation attribute)
        let sections = oracleSections;
        let currentPresentation = 'oracle'; // Will be auto-detected // 'oracle' or 'book'

        // Notes Management (for presenter view only)
        const API_BASE_URL = 'http://localhost:3000/api';
        const PRESENTATION_ID = 'merged-portfolio-2025';
        let notesData = {}; // Cache of all notes for presenter view

        // Load all notes from MongoDB on startup
        async function loadAllNotes() {
            try {
                const response = await fetch(`${API_BASE_URL}/notes/${PRESENTATION_ID}`);
                const notes = await response.json();

                // Store in cache
                notes.forEach(note => {
                    notesData[note.slideIndex] = note.notes;
                    // Also set data-notes attribute for presenter view compatibility
                    if (slides[note.slideIndex]) {
                        slides[note.slideIndex].dataset.notes = note.notes;
                    }
                });

                console.log(`Loaded ${notes.length} notes from MongoDB`);
            } catch (error) {
                console.warn('Could not load notes from MongoDB:', error);
            }
        }

        // Function to rebuild navbar based on current presentation
        function rebuildNavbar() {
            // Determine which presentation we're in by checking the current slide's data attribute
            const currentSlideElement = slides[currentSlide];
            const slidePresentation = currentSlideElement?.getAttribute('data-presentation') || 'oracle';
            
            console.log(`ðŸ” Current slide ${currentSlide}: presentation="${slidePresentation}"`);
            
            // Only rebuild if presentation changed (or first time)
            if (slidePresentation !== currentPresentation || navbar.children.length === 0) {
                console.log(`ðŸ”„ Switching from ${currentPresentation} to ${slidePresentation} presentation`);
                currentPresentation = slidePresentation;
                sections = currentPresentation === 'oracle' ? oracleSections : bookSections;

                // Clear and rebuild navbar
                navbar.innerHTML = '';

                // Build navbar buttons
                sections.forEach((section, index) => {
                    if (section.name) { // Only create button if section has a name
                        const button = document.createElement('button');
                        button.textContent = section.name;
                        button.onclick = () => goToSection(index);
                        navbar.appendChild(button);
                    }
                });

                console.log(`âœ… Navbar rebuilt for ${currentPresentation} presentation with ${navbar.children.length} buttons`);
            }
        }

        function updateSlide() {
            const currentSlideElement = slides[currentSlide];
            const presentation = currentSlideElement?.getAttribute('data-presentation') || 'unknown';
            console.log(`â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”`);
            console.log(`ðŸ“ SLIDE ${currentSlide + 1}/${totalSlides}`);
            console.log(`ðŸ“Š Presentation: ${presentation}`);
            console.log(`ðŸŽ¯ Current navbar: ${currentPresentation}`);

            slides.forEach((slide, index) => {
                slide.classList.remove('active');
                if (index === currentSlide) {
                    slide.classList.add('active');
                }
            });

            // Update slide counter
            slideCounter.textContent = `${currentSlide + 1} / ${totalSlides}`;

            // Hide navbar for divider slides
            if (currentSlideElement.dataset.hideNavbar === 'true') {
                navbar.style.display = 'none';
            } else {
                navbar.style.display = 'flex';
            }

            // Rebuild navbar if presentation changed
            rebuildNavbar();

            // Update active section in navbar
            updateActiveSection();

            // Notify presenter window of slide change
            if (presenterWindow && !presenterWindow.closed) {
                presenterWindow.postMessage({
                    type: 'slide-change',
                    slide: currentSlide
                }, '*');
            }
        }

        function updateActiveSection() {
            const navButtons = navbar.querySelectorAll('button');
            let activeSectionIndex = -1;
            let buttonIndex = 0;

            // Determine which section we're in
            for (let i = 0; i < sections.length; i++) {
                if (currentSlide >= sections[i].start && currentSlide <= sections[i].end) {
                    activeSectionIndex = i;
                    break;
                }
            }

            // Update button styles - accounting for sections without buttons (intro slides)
            navButtons.forEach((button, idx) => {
                button.classList.remove('active-section');
            });

            // Find which button corresponds to active section
            let btnIdx = 0;
            for (let i = 0; i < sections.length; i++) {
                if (sections[i].name) { // This section has a button
                    if (i === activeSectionIndex) {
                        navButtons[btnIdx]?.classList.add('active-section');
                        break;
                    }
                    btnIdx++;
                }
            }
        }

        function nextSlide() {
            if (currentSlide < totalSlides - 1) {
                currentSlide++;
                updateSlide();
                // Broadcast to other windows
                if (syncChannel) {
                    console.log('ðŸ“¤ Broadcasting next slide:', currentSlide);
                    syncChannel.postMessage({ type: 'slide-change', slide: currentSlide });
                }
            }
        }

        function prevSlide() {
            if (currentSlide > 0) {
                currentSlide--;
                updateSlide();
                // Broadcast to other windows
                if (syncChannel) {
                    console.log('ðŸ“¤ Broadcasting prev slide:', currentSlide);
                    syncChannel.postMessage({ type: 'slide-change', slide: currentSlide });
                }
            }
        }

        function goToSection(sectionNum) {
            if (sectionNum >= 0 && sectionNum < sections.length) {
                currentSlide = sections[sectionNum].start;
                updateSlide();
                // Broadcast to other windows
                syncChannel.postMessage({ type: 'slide-change', slide: currentSlide });
            }
        }

        // Presenter view variables
        let presenterWindow = null;
        let startTime = null;

        // Keyboard navigation
        document.addEventListener('keydown', (e) => {
            if (e.key === 'ArrowRight') {
                e.preventDefault();
                nextSlide();
                if (presenterWindow && !presenterWindow.closed) {
                    presenterWindow.postMessage({type: 'slide-change', slide: currentSlide}, '*');
                }
            } else if (e.key === 'ArrowLeft') {
                e.preventDefault();
                prevSlide();
                if (presenterWindow && !presenterWindow.closed) {
                    presenterWindow.postMessage({type: 'slide-change', slide: currentSlide}, '*');
                }
            } else if (e.key === 'Home') {
                e.preventDefault();
                currentSlide = 0;
                updateSlide();
                if (presenterWindow && !presenterWindow.closed) {
                    presenterWindow.postMessage({type: 'slide-change', slide: currentSlide}, '*');
                }
            } else if (e.key === 'End') {
                e.preventDefault();
                currentSlide = totalSlides - 1;
                updateSlide();
                if (presenterWindow && !presenterWindow.closed) {
                    presenterWindow.postMessage({type: 'slide-change', slide: currentSlide}, '*');
                }
            } else if (e.key === 'p' || e.key === 'P') {
                e.preventDefault();
                openPresenterView();
            }
        });

        // Switch current window to presenter mode (for the screen that should show presenter view)
        function switchToPresenterMode() {
            // Redirect this window to presenter view mode
            const currentUrl = new URL(window.location.href);
            currentUrl.searchParams.set('mode', 'presenter');
            currentUrl.searchParams.set('slide', currentSlide);
            window.location.href = currentUrl.toString();
        }

        // Open presenter view in new window (old behavior - still available)
        async function openPresenterView() {
            if (presenterWindow && !presenterWindow.closed) {
                presenterWindow.focus();
                return;
            }

            // Make sure notes are loaded first
            if (Object.keys(notesData).length === 0) {
                console.log('Loading notes before opening presenter view...');
                await loadAllNotes();
            }

            startTime = Date.now();
            presenterWindow = window.open('presenter.html', 'Presenter View', 'width=1200,height=800');

            // Wait for presenter window to be ready, then send initial state
            window.presenterMessageHandler = (event) => {
                if (event.data.type === 'presenter-ready') {
                    sendPresenterData();
                } else if (event.data.type === 'navigate') {
                    if (event.data.direction === 'next') {
                        nextSlide();
                    } else if (event.data.direction === 'prev') {
                        prevSlide();
                    }
                } else if (event.data.type === 'reset-timer') {
                    startTime = Date.now();
                } else if (event.data.type === 'note-updated') {
                    // Update note in local cache when edited from presenter view
                    const slideIndex = event.data.slide;
                    const notes = event.data.notes;
                    console.log(`Note updated from presenter for slide ${slideIndex}:`, notes);

                    notesData[slideIndex] = notes;
                    if (slides[slideIndex]) {
                        slides[slideIndex].dataset.notes = notes;
                    }
                }
            };
            window.addEventListener('message', window.presenterMessageHandler);

            // Also send after a delay as backup
            setTimeout(() => {
                sendPresenterData();
            }, 500);

            // Update timer every second
            if (!window.presenterTimerInterval) {
                window.presenterTimerInterval = setInterval(() => {
                    if (presenterWindow && !presenterWindow.closed && startTime) {
                        const elapsed = Math.floor((Date.now() - startTime) / 1000);
                        presenterWindow.postMessage({type: 'timer', elapsed: elapsed}, '*');
                    }
                }, 1000);
            }
        }

        function sendPresenterData() {
            if (presenterWindow && !presenterWindow.closed) {
                console.log('=== SENDING PRESENTER DATA ===');
                console.log('notesData object:', notesData);
                console.log('notesData entries:', Object.keys(notesData).length);

                const slidesWithNotes = Array.from(slides).map((slide, index) => {
                    const fromCache = notesData[index];
                    const fromDataset = slide.dataset.notes;
                    const finalNotes = fromCache || fromDataset || '';

                    if (index < 5) {  // Debug first 5 slides
                        console.log(`Slide ${index}:`);
                        console.log('  - fromCache:', fromCache);
                        console.log('  - fromDataset:', fromDataset);
                        console.log('  - final:', finalNotes);
                    }

                    return {
                        title: slide.querySelector('.slide-title, .main-title, .divider-title')?.textContent || `Slide ${index + 1}`,
                        notes: finalNotes
                    };
                });

                console.log('Sending', slidesWithNotes.length, 'slides to presenter');
                presenterWindow.postMessage({
                    type: 'init',
                    slide: currentSlide,
                    totalSlides: totalSlides,
                    slides: slidesWithNotes
                }, '*');
            }
        }


        // Initialize
        updateSlide();
        loadAllNotes(); // Load notes from MongoDB on startup
    </script>

</body>
</html>