        }

        .author-info {
            text-align: center;
            font-size: 1.25rem;
            z-index: 10;
            line-height: 2;
        }

        .highlight-box {
            background: linear-gradient(135deg, #14535F 0%, #0f766e 50%, #14535F 100%);
            color: white;
            padding: 1.5rem;
            border-radius: 1rem;
            margin: 1rem 0;
            font-size: 1.125rem;
            box-shadow: 0 4px 6px rgba(0,0,0,0.1);
            transition: all 0.3s;
        }

        .highlight-box:hover {
            transform: scale(1.02);
            box-shadow: 0 8px 16px rgba(20,83,95,0.3);
        }

        .card-grid {
            display: grid;
            grid-template-columns: repeat(2, 1fr);
            gap: 1.5rem;
            width: 100%;
        }

        .card {
            background: white;
            padding: 1.5rem;
            border-radius: 1rem;
            box-shadow: 0 4px 6px rgba(0,0,0,0.1);
            border-top: 4px solid #14535F;
            transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);
            position: relative;
            overflow: hidden;
        }

        .card::before {
            content: '';
            position: absolute;
            top: 0;
            left: -100%;
            width: 100%;
            height: 100%;
            background: linear-gradient(90deg, transparent, rgba(20,83,95,0.1), transparent);
            transition: left 0.5s;
        }

        .card:hover::before {
            left: 100%;
        }

        .card:hover {
            transform: translateY(-8px) scale(1.02);
            box-shadow: 0 12px 24px rgba(20,83,95,0.25);
            border-top-width: 6px;
        }

        .card-title {
            font-size: 1.25rem;
            font-weight: 700;
            color: #14535F;
            margin-bottom: 0.75rem;
        }

        .card-content {
            font-size: 1rem;
            line-height: 1.6;
            color: #334155;
        }

        .tool-grid {
            display: grid;
            grid-template-columns: repeat(4, 1fr);
            gap: 2rem;
            margin-top: 2rem;
        }

        .tool-item {
            display: flex;
            flex-direction: column;
            align-items: center;
            gap: 1rem;
            background: white;
            padding: 1.5rem;
            border-radius: 1rem;
            box-shadow: 0 4px 6px rgba(0,0,0,0.1);
        }

        .tool-item img {
            height: 60px;
            object-fit: contain;
        }

        .tool-name {
            font-size: 1.125rem;
            font-weight: 600;
            color: #14535F;
        }

        .full-image-slide {
            display: flex;
            align-items: center;
            justify-content: center;
            padding: 6rem 3rem 3rem 3rem;
        }

        .full-image-slide img {
            max-width: 90%;
            max-height: 85vh;
            object-fit: contain;
            border-radius: 1rem;
            box-shadow: 0 10px 30px rgba(0,0,0,0.2);
        }

    </style>
</head>
